{
  "name": "pacific-poke",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "client": "yarn --cwd front-end start",
    "client-install": "yarn --cwd front-end install",
    "test": "mocha --require ts-node/register test/*.ts --timeout 10000 --exit",
    "server": "nodemon bin/www",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "@types/chai": "^4.1.7",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "mysql": "^2.16.0",
    "promise": "latest",
    "pug": "2.0.0-beta11",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/mysql": "^2.15.5",
    "concurrently": "^4.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6"
  }
}
